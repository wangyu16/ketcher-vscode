{
  "name": "chemical-editor",
  "displayName": "Ketcher Chemical Editor",
  "description": "Chemical structure editor for VS Code using Ketcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangyu16/ketcher-vscode.git"
  },
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "ketcher.editor",
        "displayName": "Ketcher Chemical Editor",
        "selector": [
          {
            "filenamePattern": "*.ketcher.svg"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "ketcher.viewSource",
        "title": "View Source",
        "icon": "$(code)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "ketcher.viewSource",
          "group": "navigation",
          "when": "activeCustomEditorId == ketcher.editor"
        }
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "package": "vsce package"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "ketcher-core": "^3.12.0-dev.1",
    "ketcher-react": "^3.12.0-dev.1",
    "ketcher-standalone": "^3.12.0-dev.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.80.0",
    "@types/xml2js": "^0.4.11",
    "buffer": "^6.0.3",
    "css-loader": "^7.1.2",
    "jest": "^29.7.0",
    "process": "^0.11.10",
    "source-map-loader": "^5.0.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.7.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}